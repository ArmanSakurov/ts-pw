    "adm-zip": "^0.5.12",
    "allure-playwright": "^2.15.1",
    "crypto-js": "^4.2.0",
    "eslint-plugin-import": "^2.29.1",
    "exceljs": "^4.4.0",
    "lighthouse": "^9.6.8",
    "npm": "^10.7.0",
